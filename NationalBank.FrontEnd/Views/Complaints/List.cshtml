@model IEnumerable<ComplaintsModel> 

<div class="card">
    <div class="card-body">
        <table class="table table-bordered table-striped" id="complaintsTable">
            <thead>
                <tr>
                    <th>Category</th>
                    <th>Description</th>
                    <th>Status</th>
                    <th>Created Date</th>
                </tr>
            </thead>
            <tbody id="complaintsContainer">
                 @await Html.PartialAsync("_ComplaintsPartial",Model)
            </tbody>
        </table>
    </div>
</div>
<div id="loading" class="text-center my-2" style="display:none;">
    <div class="spinner-border text-primary" role="status">
        <span class="sr-only"></span>
    </div>
</div>
<script src="~/lib/jquery/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
<script src="~/js/complaints.js" asp-append-version="true"></script>